{
  "name": "@novu/dashboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "pnpm legacy:clean && pnpm legacy:copy && vite",
    "dev": "pnpm start",
    "build": "pnpm legacy:prepare && tsc -b && vite build",
    "legacy:prepare": "pnpm legacy:clean && pnpm legacy:build && pnpm legacy:copy",
    "legacy:clean": "rimraf legacy/*",
    "legacy:build": "cd ../.. && pnpm build:web-for-dashboard",
    "legacy:copy": "cp -r ../web/build/* ./legacy",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.9.1",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@tanstack/react-query": "^4.20.4",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.439.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "7.43.9",
    "react-router-dom": "6.26.2",
    "tailwind-merge": "^2.4.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "rimraf": "^3.0.2",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "tailwindcss": "^3.4.13",
    "typescript": "5.6.2",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1",
    "vite-plugin-static-copy": "^1.0.6"
  }
}
